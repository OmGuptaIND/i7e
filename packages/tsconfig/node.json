{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "node",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["ES2022", "dom", "dom.iterable"],
    "target": "es2022",

    "moduleResolution": "Bundler",
    "module": "ES2022",
    "noEmit": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
